public enum marca {
    TOYOTA(1),
    SUZUKI(2),
    MITSUBISHI(3),
    BMW(4),
    FORD(5),
    VOLKSWAGEN(6),
    AUDI(7),
    NISSAN(8),
    JEEP(9),
    VOLVO(10);
    
    public int idMarca;

    private marca(int idMarca) {
        this.idMarca = idMarca;
    }

    public int getIdMarca() {
        return idMarca;
    }

    public void setIdMarca(int idMarca) {
        this.idMarca = idMarca;
    }
    
    
    
    
}
