public enum estilo {
    COMPACTO(1),
    PICKUP(2),
    INTERMEDIO(3),
    SUV(4),
    MINI_VAN(5),
    CONVERTIBLE(6),
    ECONOMICO(7);
    
    public int idEstilo;

    private estilo(int idEstilo) {
        this.idEstilo = idEstilo;
    }

    public int getIdEstilo() {
        return idEstilo;
    }

    public void setIdEstilo(int idEstilo) {
        this.idEstilo = idEstilo;
    }
    
}
